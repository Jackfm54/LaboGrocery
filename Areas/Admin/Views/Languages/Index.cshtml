@model IEnumerable<LaboGrocery.Models.Language>
    <h1>Languages</h1>
    <div class="mb-3">
        <a class="btn btn-primary" asp-action="Create">+ Create</a>
    </div>
    <table class="table table-striped">
        <thead><tr><th>Culture</th><th>Name</th></tr></thead>
        <tbody>
            @foreach (var l in Model) {
            <tr><td>@l.Culture</td><td>@l.DisplayName</td></tr>
 }
        </tbody>
    </table>
